{
	"env": {
			"browser": true,
			"commonjs": true,
			"es2021": true,
			"node": true
	},
	"extends": "eslint:recommended",
	"overrides": [
	],
	"parserOptions": {
			"ecmaVersion": "latest"
	},
	"rules": {
			"indent": [
					"error",
					"tab",
					{ "SwitchCase": 1 }
			],
			"linebreak-style": [
					"error",
					"unix"
			],
			"semi": [
					"error",
					"always"
			],
			"no-eval": "error"
	}
}
